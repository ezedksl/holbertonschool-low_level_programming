#include "holberton.h"

/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */
void times_table(void)
{
int a;
int b;
int x;
int n;
int m;
for (a = 0; a <= 9; a++)
{
for (b = 0; b <= 9; b++)
{
x = a * b;
if (x > 9)
{
n = (x / 10);
m = (x % 10);
_putchar(n + '0');
_putchar(m + '0');
}
else
{
_putchar(x);
 }
 }
_putchar('\n');
}
}
